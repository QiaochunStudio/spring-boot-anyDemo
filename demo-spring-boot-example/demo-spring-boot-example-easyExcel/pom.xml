            <groupId>mysql</groupId>
            <artifactId>mysql-connector-java</artifactId>
        </dependency>

        <dependency>
            <groupId>com.hjt</groupId>
            <artifactId>demo-spring-boot-generator</artifactId>
        </dependency>

        <dependency>
            <groupId>com.hjt</groupId>
